{
  "name": "obsidian-functionplot",
  "version": "1.2.1",
  "description": "A plugin for displaying mathematical functions in obsidian.md.",
  "repository": "https://github.com/leonhma/obsidian-functionplot",
  "author": "leonhma",
  "type": "module",
  "license": "MIT",
  "private": true,
  "scripts": {
    "clean:build": "rimraf main.js main.js.map",
    "build": "yarn clean:build && webpack",
    "build:prod": "yarn clean:build && yarn webpack --env mode=production",
    "build:release": "yarn clean:build && yarn webpack --env mode=release",
    "test": "node .github/integrity-check.mjs",
    "format": "yarn prettier --write .",
    "format:check": "yarn prettier --check .",
    "lint": "yarn eslint ."
  },
  "precommit": [
    "test",
    "format",
    "lint"
  ],
  "devDependencies": {
    "@sentry/cli": "^2.5.2",
    "@types/qrcode": "^1.5.0",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "2.7.1",
    "rimraf": "^3.0.2",
    "source-map-loader": "^4.0.0",
    "ts-loader": "^9.3.1",
    "typescript": "^4.8.2",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "@sentry/browser": "^7.12.1",
    "@sentry/tracing": "^7.12.1",
    "function-plot": "^1.22.9-0",
    "html-to-image": "^1.10.6",
    "obsidian": "0.12.2"
  }
}
